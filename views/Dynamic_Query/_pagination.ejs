<div class="page-con">
    <a class="<%=page == 1 ? 'disable' : ""%>" href="<%=path%>?page=1" id="start"   ><<</a>
    <a class="<%=page <= 1 ? 'disable': "" %>" href="<%=path%>?page=<%=page-1 %>" id="dec"><</a>
    <p><%=page %></p>
    <a class="<%=page >= totalpage ? 'disable' : "" %>" href="<%=path%>?page=<%=page+1 %>" id="inr">></a>
    <a class="<%=page == totalpage ? 'disable' : "" %>" href="<%=path%>?page=<%=totalpage %>" id="end">>></a>   
</div>